<table class="table table-striped table-sm">
  <thead>
    <% for(let i = 0; i < cols.length; i++) {%>
      <th><%= cols[i] %></th>
    <% } %>
  </thead>
  <tbody>
    <% for (let i = 0; i < results.length; i++) { %>
      <tr>
        <% for (let j = 0; j < cols.length; j++) { %>
          <% if (typeof results[i][cols[j]] === 'object') { %>
            <td><%= JSON.stringify(results[i][cols[j]]) %></td>
          <% } else { %>
              <td><%= results[i][cols[j]] %></td>
          <% } %>
        <% } %>
      </tr>
    <% } %>
  </tbody>
</table>